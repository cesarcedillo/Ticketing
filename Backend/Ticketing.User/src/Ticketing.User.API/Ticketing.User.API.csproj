		</PackageReference>
		<PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
		<PackageReference Include="Ticketing.Core.Observability" Version="1.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Ticketing.User.Application\Ticketing.User.Application.csproj" />
		<ProjectReference Include="..\Ticketing.User.Infrastructure\Ticketing.User.Infrastructure.csproj" />
	</ItemGroup>

</Project>
